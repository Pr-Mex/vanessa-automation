	StepDefinition = StepDefinition + "(" + СтрокаПараметров + ")";
	
	//Если СтрДлина(args) > 0 Тогда
	//	args = Лев(args,СтрДлина(args)-1);//убрали запятую
	//КонецЕсли;
	//Сообщить("args="+args);
	
	
	Возврат StepDefinition;
	//Сообщить(стр);
КонецФункции

