	МассивРасхождений = ПолучитьРасхожденияТД(ТабДок, ТабПарам,"точный");
	
	Если МассивРасхождений.Количество() > 0 Тогда
		СтрокаОшибки = Вычислить("СтрШаблон(""Ошибка при поиске значений в макете %1:"", ИмяЭлементаФормы)");
		МассивРасхождений.Вставить(0, СтрокаОшибки);
		СтрокаОшибки = Вычислить("СтрСоединить(МассивРасхождений, Символы.ВК)");
		ВызватьИсключение СтрокаОшибки;
	КонецЕсли;
	
