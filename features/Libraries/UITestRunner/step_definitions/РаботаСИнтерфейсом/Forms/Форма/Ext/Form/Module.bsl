					ТекстСообщения = СтрЗаменить(ТекстСообщения,"%2",ВыбираемоеЗначение); 
					ТекстСообщения = СтрЗаменить(ТекстСообщения,"%3",ТекТекст); 
					ВызватьИсключение ТекстСообщения;
				КонецЕсли;	
			КонецЕсли;	
			
			Попытка
