		ТекстСообщения = Локализовать("Не найден активный элемент формы.");
		ВызватьИсключение ТекстСообщения;
	КонецЕсли;	 
	
	Если ТипЗнч(ТекущееПоле) = Тип("ТестируемаяТаблицаФормы") Тогда
		ТекущееПоле = ТекущееПоле.ПолучитьТекущийЭлемент();
	КонецЕсли;	 
	
	Возврат ТекущееПоле; 
