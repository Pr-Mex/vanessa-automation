	ПараметрыОтбора = Новый Структура;
	ПараметрыОтбора.Вставить("Имя", ИмяПрофиля);
	
	НайденныеСтроки = ПрофилиПоискаКартинокСТекстом.НайтиСтроки(ПараметрыОтбора);
