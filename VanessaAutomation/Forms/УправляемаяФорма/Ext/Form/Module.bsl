	Команда = "tasklist /FI ""PID eq " + Формат(PID, "ЧГ=; ЧН=0") +  """ > """ + ЛогФайл + """";
	ВыполнитьКомандуОСБезПоказаЧерногоОкна(Команда);
	
	Если НЕ ФайлСуществуетКомандаСистемы(ЛогФайл) Тогда
