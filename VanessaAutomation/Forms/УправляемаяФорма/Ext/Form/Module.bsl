				ЕстьВертЧерта = Ложь;
				Если Найти(Стр, "|") > 0 Тогда
					ЕстьВертЧерта = Истина;
				КонецЕсли;	 
				Если ЕстьВертЧерта Тогда
					УзелКод = ДокументHTML.СоздатьЭлемент("pre"); 
					УзелКод.ДобавитьДочерний(ТекстовыйУзел);
					УзелАбзац.ДобавитьДочерний(УзелКод);
				Иначе
