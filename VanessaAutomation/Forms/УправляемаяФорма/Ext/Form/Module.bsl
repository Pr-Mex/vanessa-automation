	Год   = 0;
	Год2000 = 2000;
	Век = 100;
	Месяц = 0;
	День  = 0;
	
	Поз  = Найти(Стр, ".");
	День = Число(Лев(Стр, Поз - 1));
	Стр = Сред(Стр, Поз + 1);
	
	Поз   = Найти(Стр, ".");
	Месяц = Число(Лев(Стр, Поз - 1));
