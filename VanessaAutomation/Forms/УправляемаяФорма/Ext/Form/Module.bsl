		Результат = НайденныеСтроки[0];
	Иначе	
		ПараметрыОтбора = Новый Структура;
		ПараметрыОтбора.Вставить("Синоним", ИмяКлиентаТестирования);
		
		НайденныеСтроки = ДанныеКлиентовТестирования.НайтиСтроки(ПараметрыОтбора);
