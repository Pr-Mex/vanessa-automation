	КонецЕсли;	 
	
	Текст = Новый ЧтениеТекста;
	Текст.Открыть(ЛогФайл, "UTF-8");
	ТекстФайла = Текст.Прочитать();
	Текст.Закрыть();
	УдалитьФайлыКомандаСистемы(ЛогФайл);
	
	Если Найти(ТекстФайла, Формат(PID, "ЧГ=; ЧН=0")) > 0 Тогда
		Возврат Истина;
	КонецЕсли;	 
	
	Возврат Ложь;
	
	#КонецЕсли
	
