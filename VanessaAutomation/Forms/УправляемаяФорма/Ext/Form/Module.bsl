	Иначе
		СлужебнаяСтрока = МассивТекстФичи.Найти("#[autodoc.ignore.featureslide]");
		Нашли = ?(СлужебнаяСтрока = Неопределено, Ложь, Истина);
	КонецЕсли;
	
	Если Нашли Тогда
		Сценарий.Вставить("ИгнорироватьСлайдСЗаголовкомФичиВАвтоинструкции", Истина);
