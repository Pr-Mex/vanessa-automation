	Иначе
		ВызватьИсключение ТипКлиента;
	КонецЕсли;
	СтартМобильногоКлиента(ВидПриложения);
	Sleep(1);
	СтрокаЗапуска = "adb shell am broadcast --user current -n " + ВидПриложения + "/com.e1c.mobile.Starter -a com.e1c.mobile.START_CMD -e cmd '%1'";
