		Если СвойстваПроцесса = Неопределено Тогда
			Возврат Ложь;
		Иначе	
			Возврат Истина;
		КонецЕсли;	 
	КонецЕсли;	 
	
	#Если НЕ ВебКлиент Тогда
		
		Если ЭтоLinux Тогда
			
			Результат = ПроцессОССуществуетLinux(PID);
			
		Иначе
			
			Результат = ПроцессОССуществуетWindows(PID);
			
		КонецЕсли;
	
	#КонецЕсли
	
	Возврат Результат;
	
