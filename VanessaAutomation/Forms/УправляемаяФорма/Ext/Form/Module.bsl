	Если Лев(ИмяПоля, 2) = "\!" Тогда
		ИмяПоля = Сред(ИмяПоля, 2);
		ИскатьПоИмени = Ложь;
		Возврат;
	ИначеЕсли Лев(ИмяПоля, 1) = "!" Тогда
		ИмяПоля = Сред(ИмяПоля, 2);
		ИскатьПоИмени = Истина;
		Возврат;
	КонецЕсли;	 
	ИскатьПоИмени = Ложь;
КонецПроцедуры

